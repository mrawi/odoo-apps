<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_move_form_sheets" model="ir.ui.view">
        <field name="name">view_move_form_sheets</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page[@name='invoice_tab']/field[@name='invoice_line_ids']/tree/field[@name='quantity']" position="before">
                <field name="sheet_num"/>
            </xpath>
        </field>
    </record>
</odoo>