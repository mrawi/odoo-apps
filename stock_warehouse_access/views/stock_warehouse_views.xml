<?xml version="1.0" encoding="UTF-8" ?>

<odoo>
    <record id="stock_warehouse_access_user_form" model="ir.ui.view">
        <field name="name">Warehouse Access User Form View</field>
        <field name="model">stock.warehouse</field>
        <field name="inherit_id" ref="stock.view_warehouse"/>
        <field name="arch" type="xml">
            <xpath expr="//page[@name='warehouse_config']/group" position="inside">
                <group name="group_user_access" string="User Access" groups="stock.group_stock_multi_warehouses">
                    <field name="allowed_user_ids" widget="many2many_tags"/>
                </group>
            </xpath>
        </field>
    </record>

</odoo>